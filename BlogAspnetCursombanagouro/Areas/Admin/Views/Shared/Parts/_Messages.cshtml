@if (ViewBag.SUCCESS != null)
{
    <div class="row-fluid">
        <div class="alert alert-success">
            <button type="button" class="close" data-dismiss="alert">&times;</button>
            <h4>Eba!!</h4>
            @ViewBag.SUCCESS
        </div>
    </div>
}
@if (ViewBag.ERROR != null)
{
    <div class="row-fluid">
        <div class="alert alert-error">
            <button type="button" class="close" data-dismiss="alert">&times;</button>
            <h4>Eita!</h4>
            @ViewBag.ERROR
        </div>
    </div>
}